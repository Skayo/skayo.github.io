{
	"name": "skayo-website",
	"version": "1.0.0",
	"scripts": {
		"dev": "parcel views/index.hbs --no-cache",
		"build": "parcel build views/index.hbs --no-source-maps --no-cache"
	},
	"dependencies": {
		"uikit": "^3.5.1"
	},
	"devDependencies": {
		"@inventory/parcel-plugin-handlebars": "^1.0.0",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-ogimage": "^1.2.0",
		"parcel-plugin-pwa-manifest": "^3.0.7",
		"sass": "^1.26.7"
	},
	"pwaManifest": {
		"name": "Skayo",
		"description": "Web Designer & Developer from Austria",
		"themeColor": "black",
		"display": "minimal-ui",
		"generateIconOptions": {
			"baseIcon": "./assets/img/logo.svg",
			"sizes": [
				192,
				512
			],
			"resize": "contain",
			"appleTouchIconPadding": 10,
			"generateFavicons": true,
			"formats": {}
		}
	}
}
